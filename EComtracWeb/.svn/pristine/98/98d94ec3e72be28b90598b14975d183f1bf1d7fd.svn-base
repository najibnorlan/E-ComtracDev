	var rpyparam12 = document.getElementById('rpyparam12').innerHTML; 
	var prevYear = document.getElementById('prevYear').innerHTML; 
	
var ctx = document.getElementById('revenue'); 								
var myChart = new Chart(ctx, { 														
	type: 'line', 																		
	    data: { 																		
