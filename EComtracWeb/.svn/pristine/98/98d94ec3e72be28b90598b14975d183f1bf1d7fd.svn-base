			                      const stackedValues = ctx.chart.data.datasets.map((ds) => ds.data[ctx.dataIndex]);
			                      const dsIdxLastVisibleNonZeroValue = stackedValues.reduce(
			                        (prev, curr, i) =>
			                          !!curr && !ctx.chart.getDatasetMeta(i).hidden ? Math.max(prev, i) : prev,
			                        0
			                      );
			                      const sum = stackedValues
			                        .filter((ds, i) => !ctx.chart.getDatasetMeta(i).hidden)
			                        .reduce((s, v) => s + v, 0);

			                      // Calculate the y-axis scale value for positioning
			                      const scale = ctx.chart.scales['y'];

			                      // Check if the sum is greater than the highest y-axis value
			                      if (sum > scale.max) {
